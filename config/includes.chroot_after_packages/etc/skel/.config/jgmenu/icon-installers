Install Numix Circle Icons,  x-terminal-emulator -T 'Numix Circle Icon Theme Installer' -e bash -c "numix-installer; echo $'\n\nPress any key to close.'; read -srn1", applications-utilities
Install Faenza Icons,      x-terminal-emulator -T 'Faenza Icon Theme Installer' -e bash -c "faenza-installer; echo $'\n\nPress any key to close.'; read -srn1",        applications-utilities
Install Paper Icons,       x-terminal-emulator -T 'Paper Icon Theme Installer' -e bash -c "paper-installer; echo $'\n\nPress any key to close.'; read -srn1",          applications-utilities
