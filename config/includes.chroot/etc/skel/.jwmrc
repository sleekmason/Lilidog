    <!-- Options for program groups. -->
    <Group>
        <Option>tiled</Option>
        <Option>aerosnap</Option>
    </Group>
    <Group>
        <Class>Pidgin</Class>
        <Option>sticky</Option>
    </Group>
    <Group>
        <Name>xterm</Name>
        <Option>vmax</Option>
    </Group>
    <Group>
        <Name>xclock</Name>
        <Option>drag</Option>
        <Option>notitle</Option>
    </Group>
    
    <Group>
 <Class>URxvt</Class>
 <Option>iignore</Option>
</Group>
    <!-- Tray at the bottom. -->
    <Tray x="0" y="-1" height="24" autohide="off">

        <Spacer width="4"/>
		<TrayButton icon="/usr/share/icons/ld-icons/Cascade.png">exec:winfuncs cascade</TrayButton>
        <Spacer width="4"/>
        <TrayButton icon="/usr/share/icons/ld-icons/down.png">showdesktop</TrayButton>
        <Spacer width="4"/>
		<TrayButton icon="/usr/share/icons/ld-icons/hotcorners.png">exec:toggle.lwa</TrayButton>
		<Spacer width="10"/>
        <Pager labeled="true"/>
        <Spacer width="6"/>

        <TaskList maxwidth="120"/>

                <Dock/>
        <Spacer width="12"/>
        <Clock format="%d %b %H:%M"><Button mask="123">exec:urxvt -e man strftime</Button></Clock>
		<Spacer width="6"/>
		
		<TrayButton icon="/usr/share/icons/ld-icons/exit.png">exec:ld-logout</TrayButton>
		<Spacer width="2"/>
    </Tray>

    <!-- Visual Styles -->
    <WindowStyle>
        <Font>LiberationSans-11</Font>
        <Width>4</Width>
        <Height>21</Height>
        <Corner>5</Corner>
        <Foreground>#95E7F0</Foreground>
