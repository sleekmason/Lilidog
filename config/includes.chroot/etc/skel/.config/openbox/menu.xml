					<execute>gksudo pcmanfm</execute>
				</action>
			</item>
			<item label="Root Lxterminal">
				<action name="Execute">
					<execute>gksudo lxterminal</execute>
				</action>
			</item>
			<item label="SmPlayer">
				<action name="Execute">
					<execute>smplayer</execute>
				</action>
			</item>
			<item label="Gpicview">
				<action name="Execute">
					<execute>gpicview</execute>
