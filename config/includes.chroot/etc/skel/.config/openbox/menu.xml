		<menu id="root-menu-21510" label="Configuration">
			<item label="LxSession Autostart">
				<action name="Execute">
					<execute>lxsession-default-apps</execute>
				</action>
			</item>
			<item label="Power Manager">
				<action name="Execute">
					<execute>xfce4-power-manager-settings</execute>
