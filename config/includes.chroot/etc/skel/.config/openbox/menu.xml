					<execute>gnome-screenshot -i</execute>
				</action>
			</item>
			<item label="ClipIt Toggle">
				<action name="Execute">
					<execute>toggle.clipit</execute>
				</action>
			</item>
			<item label="Conky Toggle">
				<action name="Execute">
					<execute>toggle.conky</execute>
				</action>
			</item>
			<item label="Compton Toggle">
				<action name="Execute">
					<execute>toggle.compton</execute>
				</action>
			</item>
			<item label="Ld-Hotcorners Toggle">
