			<menu icon="/usr/share/icons/gnome/48x48/categories/preferences-system.png" id="root-menu-21510" label="Configuration">
				<item icon="/usr/share/icons/gnome/48x48/categories/applications-system.png" label="Power Manager">
					<action name="Execute">
						<execute>xfce4-power-manager-settings</execute>
					</action>
				</item>
				<item icon="/usr/share/icons/gnome/48x48/apps/preferences-desktop-theme.png" label="Lx-Appearance">
