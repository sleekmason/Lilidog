				<execute>urxvt</execute>
			</action>
		</item>
		<separator/>
		<separator/>
		<item icon="/usr/share/icons/ld-icons/Thunar.png" label="Thunar">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<!-- This requires the presence of the 'openbox-menu' package to work -->
		<separator/>
		<menu icon="/usr/share/icons/gnome/48x48/categories/applications-other.png" id="root-menu-594711" label="Quick Grab">
			<item icon="/usr/share/pixmaps/geany.xpm" label="Geany">
				<action name="Execute">
					<execute>geany</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/gnome/48x48/apps/system-file-manager.png" label="Pcmanfm">
