					<execute>mousepad</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/gnome/48x48/apps/accessories-dictionary.png" label="Newsboat">
				<action name="Execute">
					<execute>urxvt -g 120x40+120+100 -e newsboat</execute>
				</action>
			</item>
			<item icon="/usr/share/icons/gnome/48x48/actions/edit-find.png" label="Catfish File Search">
				<action name="Execute">
