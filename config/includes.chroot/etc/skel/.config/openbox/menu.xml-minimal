						<execute>lxappearance</execute>
					</action>
				</item>
				<item icon="/usr/share/icons/gnome/48x48/categories/preferences-desktop-peripherals.png" label="Openbox Menu">
					<action name="Execute">
						<execute>obmenu</execute>
					</action>
				</item>
				<item icon="/usr/share/pixmaps/obconf.png" label="Openbox Conf">
					<action name="Execute">
						<execute>obconf</execute>
					</action>
				</item>
				<item icon="/usr/share/icons/ld-icons/compton.png" label="Compton Conf">
					<action name="Execute">
