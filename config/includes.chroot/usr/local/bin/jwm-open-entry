#!/bin/bash
# Small script to change to openbox from jwm during session.
pkill -f stalonetray
pkill -f plank
pkill -x jwm
openbox-lxde & toggle.monster & feh --bg-scale /home/wallpaper/linux2.png
pkill -f ld-hotcorners &
pkill -f volumeicon &
killall compton ; compton &
killall nm-applet ; nm-applet &
killall conky ; conky &
xfce4-power-manager